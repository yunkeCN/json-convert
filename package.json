{
  "name": "json-convert",
  "version": "1.0.0",
  "description": "A project with mocha-typescript enabled",
  "main": "index.js",
  "scripts": {
    "pretest": "tsc",
    "test": "nyc mocha",
    "watch": "mocha-typescript-watch",
    "prepare": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+git@github.com:yunkeCN/json-convert.git"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/yunkeCN/json-convert/issues"
  },
  "homepage": "https://github.com/yunkeCN/json-convert/blob/HEAD/README.md",
  "devDependencies": {
    "@types/chai": "*",
    "@types/mocha": "*",
    "@types/node": "^8.0.46",
    "chai": "*",
    "es6-shim": "^0.35.3",
    "mocha": "*",
    "mocha-typescript": "*",
    "nyc": "*",
    "source-map-support": "*",
    "ts-node": "^3.3.0",
    "typescript": "*"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 80,
    "statements": 80,
    "functions": 80,
    "branches": 80,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "test/**/*.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "all": true
  }
}
